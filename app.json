{
  "expo": {
    "name": "com.flagsmaya",
    "slug": "flags",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/globe.png",
    "scheme": "flags",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mayagwright20.flags",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "backgroundColor": "#ffffff"
    },
    "android": {
      "backgroundColor": "#ffffff",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/globe.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/globe.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/globe.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "85ee03ab-2549-439b-920a-8735c0ca500a"
      }
    }
  }
}
